{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "fortnite-content-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-06",
  "dev": {
    "port": 8181,
    "local_protocol": "http"
  },
   "env": {
    "staging": {
      "name": "fortnite-content-api-staging"
    },
    "production": {
      "name": "fortnite-content-api-production"
    }
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  }
}